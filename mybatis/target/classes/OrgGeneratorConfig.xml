<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<classPathEntry location="mysql-connector-java-5.1.6.jar" />

	<context id="mysqlTest" targetRuntime="MyBatis3">

		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />

		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/infodb" userId="lpdb"
			password="lpdb">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.lpdb.persistance.org.entity"
			targetProject="mybatis/src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.lpdb.persistance.org.mapper"
			targetProject="mybatis/src">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.lpdb.persistance.org.mapper" targetProject="mybatis/src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="organizations" enableSelectByExample="false"
			enableDeleteByExample="false" enableCountByExample="false"
			enableUpdateByExample="false" selectByExampleQueryId="false">
		</table>
		<table tableName="organizations_in_cities" enableSelectByExample="false"
			enableDeleteByExample="false" enableCountByExample="false"
			enableUpdateByExample="false" selectByExampleQueryId="false">
		</table>
		<table tableName="organizations_types" enableSelectByExample="false"
			enableDeleteByExample="false" enableCountByExample="false"
			enableUpdateByExample="false" selectByExampleQueryId="false">
		</table>

	</context>
</generatorConfiguration>